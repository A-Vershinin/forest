{
  "name": "forest-portfolio",
  "version": "1.0.0",
  "private": true,
  "description": "Start template for new projects",
  "author": "Anatoliy Vershinin",
  "main": "gulpfile.js",
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "license": "ISC",
  "scripts": {
    "test": "npm run editorconfig && npm run stylelint && npm run eslint",
    "fix": "npm run editorconfigfix & npm run stylelintfix & npm run eslintfix",
    "editorconfig": "editorconfig-cli",
    "editorconfigfix": "editorconfig-tools fix app",
    "ghpages": "git subtree push --prefix build origin gh-pages",
    "deploy": "npm run build && gh-pages -d build",
    "codestyle": "npm run editorconfig && npm run stylelint && npm run eslint",
    "stylelint": "stylelint app/styles/**/*.scss --syntax scss",
    "stylelintfix": "stylelint app/styles/**/*.scss --fix",
    "eslint": "eslint app/js/**/*.js",
    "eslintfix": "eslint app/js/**/*.js --fix",
    "start": "set NODE_ENV=development&& gulp",
    "build": "set NODE_ENV=production&& gulp"
  },
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "^1.x",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "babelify": "^8.0.0",
    "browser-sync": "^2.18.6",
    "browserify": "^15.2.0",
    "css-mqpacker": "^6.0.1",
    "del": "^3.0.0",
    "editorconfig-tools": "^0.1.1",
    "eslint": "^4.13.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-plugin-import": "^2.8.0",
    "gh-pages": "^1.1.0",
    "gulp": "git+https://git@github.com/gulpjs/gulp.git#4.0",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-babel": "^7.0.0",
    "gulp-cheerio": "^0.6.2",
    "gulp-concat": "^2.6.1",
    "gulp-concat-css": "^2.2.0",
    "gulp-copy": "^1.0.1",
    "gulp-css-unit": "^1.2.2",
    "gulp-csscomb": "^3.0.8",
    "gulp-csso": "^3.0.0",
    "gulp-eslint": "^4.0.0",
    "gulp-html-minifier": "^0.1.8",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^3.2.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-notify": "^3.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^7.0.0",
    "gulp-pug": "^3.0.3",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.6.1",
    "gulp-sass": "3.1.x",
    "gulp-sourcemaps": "2.6.1",
    "gulp-stylelint": "^5.0.0",
    "gulp-svg-sprite": "^1.3.1",
    "gulp-svgmin": "^1.2.2",
    "gulp-uglifyes": "^0.1.3",
    "gulp-uncss": "^1.0.6",
    "gulp-webp": "^2.3.0",
    "gulp.spritesmith": "^6.7.0",
    "imagemin-jpegtran": "^5.0.2",
    "imagemin-optipng": "^5.2.1",
    "imagemin-pngquant": "^5.0.0",
    "merge-stream": "^1.0.1",
    "pixel-glass": "^1.0.13",
    "postcss-reporter": "^5.0.0",
    "stylelint": "^8.3.1",
    "stylelint-config-htmlacademy": "^0.1.x",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "dependencies": {
    "svg4everybody": "^2.1.9"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "*.html",
    "*.json",
    "*.js",
    "js/**/*.js",
    "img/**/*.svg",
    "styles/**/*.{sass,scss}"
  ],
  "engines": {
    "node": "7.9"
  },
  "babel": {
    "ignore": [
      "node_modules/**/*"
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current",
            "browsers": [
              "last 2 versions",
              "safari >= 7"
            ],
            "modules": false
          }
        }
      ],
      "stage-3"
    ],
    "plugins": [
      "transform-runtime",
      [
        "transform-es2015-modules-commonjs",
        {
          "allowTopLevelThis": true
        }
      ]
    ]
  }
}
